{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "serve_id": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "hours": {
      "type": "decimal",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "available_workspace_qty": {
      "type": "integer",
      "required": true
    },
    "people_qty": {
      "type": "integer",
      "required": true
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "service"
    },
    "time_slots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::time-slot.time-slot",
      "mappedBy": "service"
    }
  }
}
